{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "indent": "off",
    "eol-last": "off",
    "object-curly-newline": "off",
    "import/no-extraneous-dependencies": 0,
    "linebreak-style": 0,
    "no-tabs": 0,
    "operator-linebreak": 0,
    "no-underscore-dangle": 0,
    "space-before-function-paren": 0,
    "no-console": 0
  }
}
